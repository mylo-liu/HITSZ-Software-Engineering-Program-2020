<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bookkeepingapp">

    <uses-permission android:name="android.permission.VIBRATE" />
    <application
        android:name="com.example.bookkeepingapp.LitePalApplication"
        android:allowBackup="true"
        android:icon="@mipmap/addcircle"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/addcircle"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.bookkeepingapp.MainFunction.Activity_3"
                  android:windowSoftInputMode="adjustPan"></activity>
        <activity android:name="com.example.bookkeepingapp.Checking.Activity_2" />
        <activity android:name="com.example.bookkeepingapp.Setting.Activity_1" />
        <activity android:name="com.example.bookkeepingapp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>